[user]
	name = Wil Hall
	email = wil@wilhall.com
	signingkey = A4B3531B2282D01EE7AF6C616CB50AFCD54B3555
[core]
	editor = nvim
	excludesfile = /Users/wilhall/.gitignore
	trustctime = false
[push]
	default = current
[pack]
	windowMemory = 10m
	packSizeLimit = 20m
[branch]
	autosetuprebase = always
[pull]
	rebase = true
	default = current
[difftool]
	prompt = false
[alias]
	wdiff = diff -w --word-diff=color
	set-upstream = !git branch --set-upstream-to=origin/`git symbolic-ref --short HEAD`
    branches = for-each-ref --sort=-committerdate --format=\"%(color:blue)%(authordate:relative)\t%(color:red)%(authorname)\t%(color:white)%(color:bold)%(refname:short)\" refs/remotes
	whatadded = log --diff-filter=A
	ctags = !.git/hooks/ctags
[commit]
	gpgsign = true
[gpg]
	program = /usr/local/MacGPG2/bin/gpg2
[init]
	templatedir = /Users/wilhall/.dotfiles/.git_template
[tag]
	forceSignAnnotated = true
[duet "env"]
	git-author-initials = wh
	git-author-name = Wil Hall
	git-author-email = wil@wilhall.com
	mtime = 1578941094
	git-committer-initials = 
	git-committer-name = 
	git-committer-email = 
