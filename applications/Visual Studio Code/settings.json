{
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.quickSuggestionsDelay": 10,
  "stylelint.enable": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.suggestOnTriggerCharacters": true,
  "editor.parameterHints.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "window.closeWhenEmpty": false,
  "editor.emptySelectionClipboard": false,
  "files.encoding": "utf8",
  "editor.dragAndDrop": false,
  "editor.renderWhitespace": "all",
  "files.insertFinalNewline": true,
  "editor.find.seedSearchStringFromSelection": false,
  "editor.fontFamily": "Panic Sans, FiraCode-Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "window.openFilesInNewWindow": "off",
  "editor.scrollBeyondLastLine": true,
  "editor.mouseWheelScrollSensitivity": 1.5,
  "window.title": "${activeEditorFull}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "editor.useTabStops": true,
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.highlightModifiedTabs": true,
  "highlightLine.borderStyle": "dashed",
  "editor.suggest.localityBonus": true,
  "editor.accessibilitySupport": "off",
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false
  },
  "editor.parameterHints.cycle": true,
  "editor.renderControlCharacters": true,
  "editor.showFoldingControls": "always",
  "editor.minimap.showSlider": "always",
  "workbench.activityBar.visible": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 15,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.settings.useSplitJSON": true,
  "window.restoreWindows": "all",
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "debug.inlineValues": true,
  "debug.enableAllHovers": true,
  "debug.showInStatusBar": "always",
  "debug.openDebug": "openOnDebugBreak",
  "debug.toolBarLocation": "docked",
  "scm.alwaysShowActions": true,
  "scm.alwaysShowProviders": true,
  "eslint.alwaysShowStatus": true,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.highlightSelfClosing": true,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.mode.statusBar.enabled": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "terminal.integrated.fontFamily": "Meslo LG M DZ for Powerline",
  "search.location": "panel",
  "workbench.sideBar.location": "left",
  "ruby.interpreter.commandPath": "/usr/local/opt/ruby/bin/ruby",
  "solargraph.commandPath": "/usr/local/lib/ruby/gems/2.6.0/bin/solargraph",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "window.zoomLevel": 0.5,
  "jest.autoEnable": false,
  "jest.runAllTestsFirst": false,
  "jest.enableCodeLens": false,
  "jest.enableSnapshotPreviews": false,
  "jest.enableSnapshotUpdateMessages": false,
  "jest.enableInlineErrorMessages": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "eslint.autoFixOnSave": true,
  "files.associations": {
    "*.js": "javascriptreact",
    "*.ts": "javascriptreact",
    "*.jsx": "javascriptreact",
    "*.tsx": "javascriptreact",
  },
  "javascript.format.enable": false,
  "typescript.format.enable": false,
}
